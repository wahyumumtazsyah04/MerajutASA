{
  "theme": "dark",
  "background": "transparent",
  "width": 1920,
  "height": 1080,
  "scale": 2,
  "themeConfig": {
    "primaryColor": "#3498DB",
    "primaryTextColor": "#FFFFFF",
    "primaryBorderColor": "#2980B9",
    "lineColor": "#34495E",
    "sectionBkgColor": "#2C3E50",
    "altSectionBkgColor": "#34495E",
    "gridColor": "#7F8C8D",
    "textColor": "#FFFFFF",
    "taskBkgColor": "#3498DB",
    "taskTextColor": "#FFFFFF",
    "taskTextLightColor": "#FFFFFF",
    "taskTextOutsideColor": "#FFFFFF",
    "taskTextClickableColor": "#3498DB",
    "activeTaskBkgColor": "#E74C3C",
    "activeTaskBorderColor": "#C0392B",
    "section0": "#2ECC71",
    "section1": "#3498DB",
    "section2": "#9B59B6",
    "section3": "#E67E22",
    "section4": "#E74C3C"
  },
  "flowchart": {
    "htmlLabels": true,
    "curve": "basis",
    "diagramPadding": 20,
    "nodeSpacing": 50,
    "rankSpacing": 50
  },
  "sequence": {
    "diagramMarginX": 50,
    "diagramMarginY": 10,
    "actorMargin": 50,
    "width": 150,
    "height": 65,
    "boxMargin": 10,
    "boxTextMargin": 5,
    "noteMargin": 10,
    "messageMargin": 35,
    "mirrorActors": true,
    "bottomMarginAdj": 1,
    "useMaxWidth": true,
    "rightAngles": false,
    "showSequenceNumbers": false
  },
  "gantt": {
    "titleTopMargin": 25,
    "barHeight": 20,
    "barGap": 4,
    "topPadding": 50,
    "leftPadding": 75,
    "gridLineStartPadding": 35,
    "fontSize": 11,
    "fontFamily": "Open Sans, sans-serif",
    "numberSectionStyles": 4,
    "axisFormat": "%Y-%m-%d"
  },
  "journey": {
    "diagramMarginX": 50,
    "diagramMarginY": 10,
    "leftMargin": 150,
    "width": 150,
    "height": 50,
    "boxMargin": 10,
    "boxTextMargin": 5,
    "noteMargin": 10,
    "messageMargin": 35,
    "bottomMarginAdj": 1
  },
  "class": {
    "arrowMarkerAbsolute": false
  },
  "git": {
    "mainBranchName": "main",
    "showBranches": true,
    "showCommitLabel": true
  },
  "pie": {
    "textPosition": 0.75
  },
  "er": {
    "diagramPadding": 20,
    "layoutDirection": "TB",
    "minEntityWidth": 100,
    "minEntityHeight": 75,
    "entityPadding": 15,
    "stroke": "#FFFFFF",
    "fill": "#2C3E50",
    "fontSize": 12
  },
  "mindmap": {
    "padding": 10,
    "maxNodeWidth": 200
  },
  "timeline": {
    "diagramMarginX": 50,
    "diagramMarginY": 10,
    "leftMargin": 150,
    "width": 150,
    "height": 50,
    "boxMargin": 10,
    "boxTextMargin": 5,
    "noteMargin": 10,
    "messageMargin": 35,
    "bottomMarginAdj": 1
  },
  "puppeteerConfig": {
    "headless": true,
    "args": [
      "--no-sandbox",
      "--disable-setuid-sandbox",
      "--disable-dev-shm-usage",
      "--disable-extensions",
      "--disable-gpu",
      "--disable-web-security",
      "--allow-running-insecure-content"
    ]
  }
}
